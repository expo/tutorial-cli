    "@types/js-yaml": "^4.0.9",
    "arg": "^5.0.2",
    "chalk": "^5.3.0",
    "eslint": "^8.56.0",
    "eslint-config-universe": "^12.0.0",
    "getenv": "^1.0.0",
    "js-yaml": "^4.1.0",
    "open": "^10.0.3",
    "ora": "^8.0.1",
    "prettier": "^3.2.4",
    "prompts": "^2.4.2"
